use_frameworks!

target 'YHKit' do
    pod 'AFNetworking'         #网络请
    pod 'JSONModel'            #数据转模型
    pod 'SDWebImage'           #图片缓存机制
    pod 'FWCycleScrollView'    #
    pod 'MBProgressHUD'        #进度提示框
    pod 'Masonry'              #屏幕适配
    pod 'MJRefresh'            #刷新控件
    pod 'LookinServer', :configurations => ['Debug']  #可视化调试工具
end
